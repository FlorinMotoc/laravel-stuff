{
    "name": "florinmotoc/laravel-stuff",
    "description": "Laravel stuff",
    "require": {
        "php": ">=8.1",
        "florinmotoc/php-statsd-client": "dev-main",
        "florinmotoc/laravel-statsd": "dev-main",
        "florinmotoc/laravel-logs": "dev-main",
        "monolog/monolog": "*",
        "symfony/console": "*",
        "symfony/var-dumper": "*",
        "illuminate/log": "*",
        "illuminate/http": "*",
        "illuminate/queue": "*",
        "illuminate/support": "*"
    },
    "autoload": {
        "psr-4": {
            "FlorinMotoc\\LaravelStuff\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FlorinMotoc\\LaravelStuff\\LaravelStuffServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
